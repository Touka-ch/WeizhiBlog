<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weizhiblog.mapper.PvviewMapper">
    <resultMap id="BaseResultMap" type="com.weizhiblog.bean.Pvview">
        <!--@Table pvview-->
        <result column="pv" jdbcType="DECIMAL" property="pv"/>
        <result column="uid" jdbcType="INTEGER" property="uid"/>
    </resultMap>
    <sql id="Base_Column_List">
        pv,
        `uid`
    </sql>
    <insert id="insert" parameterType="com.weizhiblog.bean.Pvview">
        insert into pvview (pv, `uid`)
        values (#{pv,jdbcType=DECIMAL}, #{uid,jdbcType=INTEGER})
    </insert>
    <insert id="insertSelective" parameterType="com.weizhiblog.bean.Pvview">
        insert into pvview
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="pv != null">
                pv,
            </if>
            <if test="uid != null">
                `uid`,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="pv != null">
                #{pv,jdbcType=DECIMAL},
            </if>
            <if test="uid != null">
                #{uid,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
    <insert id="batchInsert" parameterType="map">
        insert into pvview
                (pv, `uid`)
                values
        <foreach collection="list" item="item" separator=",">
            (#{item.pv,jdbcType=DECIMAL}, #{item.uid,jdbcType=INTEGER})
        </foreach>
    </insert>
    <insert id="insertOrUpdate" parameterType="com.weizhiblog.bean.Pvview">
        insert into pvview
                (pv, `uid`)
        values (#{pv,jdbcType=DECIMAL}, #{uid,jdbcType=INTEGER})
        on duplicate key update pv    = #{pv,jdbcType=DECIMAL},
                                `uid` = #{uid,jdbcType=INTEGER}
    </insert>
    <insert id="insertOrUpdateSelective" parameterType="com.weizhiblog.bean.Pvview">
        insert into pvview
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="pv != null">
                pv,
            </if>
            <if test="uid != null">
                `uid`,
            </if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="pv != null">
                #{pv,jdbcType=DECIMAL},
            </if>
            <if test="uid != null">
                #{uid,jdbcType=INTEGER},
            </if>
        </trim>
        on duplicate key update
        <trim suffixOverrides=",">
            <if test="pv != null">
                pv = #{pv,jdbcType=DECIMAL},
            </if>
            <if test="uid != null">
                `uid` = #{uid,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
</mapper>